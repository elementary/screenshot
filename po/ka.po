msgid "Name"
msgstr "სახელი"

#: src/Widgets/SaveDialog.vala:124
msgid "File Type"
msgstr "ფაილის ტიპი"

#: src/Widgets/SaveDialog.vala:147
msgid "Folder"
msgstr "საქაღალდე"

#: src/Widgets/SaveDialog.vala:151
msgid "Select Screenshots Folder…"
msgstr "ეკრანის ანაბეჭდების საქაღალდის არჩევა…"

