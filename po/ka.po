#: src/Application.vala:21
msgid "Screenshots"
msgstr "ეკრანის ანაბეჭდები"

#: src/Application.vala:33
msgid "Capture area"
msgstr "გადასაღები ადგილი"

#: src/Application.vala:34
msgid "Capture the whole screen"
msgstr "მთელი ეკრანის გადაღება"

#: src/Application.vala:35
msgid "Capture active window"
msgstr "აქტური ფანჯრის გადაღება"

#: src/Application.vala:36
msgid "Take screenshot after specified delay"
msgstr "ანაბეჭდის მითითებული დაყოვნების შემდეგ გადაღება"

#: src/Application.vala:37
msgid "Seconds"
msgstr "წამი"

