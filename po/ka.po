msgid "Copy to Clipboard"
msgstr "ბუფერში კოპირება"

#: src/Widgets/SaveDialog.vala:172
msgid "Cancel"
msgstr "გაუქმება"

#: src/Widgets/SaveDialog.vala:174
msgid "Save"
msgstr "შენახვა"

#: src/Widgets/SaveDialog.vala:180
msgid "Filename can't be blank"
msgstr "ფაილის სახელი არ შეიძლება, ცარიელი იყოს"

#: src/Widgets/SaveDialog.vala:183
msgid "Filename can't contain “/”"
msgstr "ფაილის სახელი არ შეიძლება შეიცავდეს \"/\""

