msgid "Take Screenshot"
msgstr "Krei ekrankopion"

#: src/ScreenshotWindow.vala:111
msgid "Close"
msgstr "Fermi"

#: src/ScreenshotWindow.vala:338
msgid "Screenshot taken"
msgstr "Ekrankopio farita"

#. / TRANSLATORS: %s represents a timestamp here
#: src/ScreenshotWindow.vala:380 src/Widgets/SaveDialog.vala:89
#, c-format
msgid "Screenshot from %s"
msgstr "Ekrankopita je %s"

