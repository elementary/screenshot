#: src/Application.vala:38
msgid "Screenshots"
msgstr "Ekrankopioj"

#: src/Application.vala:44
msgid "Capture active window"
msgstr "Kapti la aktivan fenestron"

#: src/Application.vala:45
msgid "Capture area"
msgstr "Kapti areaĵon"

#: src/Application.vala:46
msgid "Capture the whole screen"
msgstr "Kapti la tutan ekranon"

#: src/Application.vala:47
msgid "Take screenshot after specified delay"
msgstr "Kapti post specifita daŭro"

#: src/Application.vala:47
msgid "Seconds"
msgstr "Sekundoj"

