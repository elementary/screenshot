#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1
export DEB_BUILD_MAINT_OPTIONS=hardening=+all

%:
	dh $@

override_dh_strip:
	dh_strip --dbgsym-migration='screenshot-tool-dbg (<< 0.1.5)'

override_dh_missing:
	dh_missing --fail-missing --list-missing
